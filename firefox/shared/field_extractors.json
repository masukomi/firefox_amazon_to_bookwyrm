{
  "title": "document.getElementById('productTitle').textContent.replace(/^\\s+|\\s+$/g, '')",
  "description": "document.querySelector('#bookDescription_feature_div > div > div ').innerHTML.replace(/<br>|<\\/?span.*?>|<\\/?p.*?>/g, '\\n').replace(/\\n{3,}/, '\\n\\n').trim()",
  "series": "document.querySelector('#seriesBulletWidget_feature_div > a:nth-child(1)').textContent.replace(/.*Part of:\\s/i, '').replace(/ \\(\\d+ books\\)\\s+/, '')",
  "languages": "productDetail(/Language/i)",
  "subjects": "productDetail(/Best Sellers Rank/i).split('#').filter(x => ! x.match(/^$|Kindle Store/)).map((x) => x.replace(/(\\d+,?)+ in /, '').replace(/\\s+\\(.*?\\)$/, ''))",
  "publishers": "productDetail(/Publisher/i)",
  "published_date_month": "stringDateToDictionary(productDetail(/Publication date/)).month",
  "published_date_day": "stringDateToDictionary(productDetail(/Publication date/)).day",
  "published_date_year": "stringDateToDictionary(productDetail(/Publication date/)).year",
  "add_author": "document.querySelector('.author').textContent.replace(/^\\s+/, '').replace(/\\s{2,}/g, ' ').replace(/\\s+\\(.*?\\).*?$/, '')",
  "cover_url": "document.getElementById('landingImage').attributes.getNamedItem('data-old-hires').value",
  "pages": "productDetail(/Print length/i).replace(/\\D+/, '')",
  "isbn_13": "productDetail(/ISBN-13/i)",
  "asin": "productDetail(/ASIN/i)"
}
